import sys

n = int(sys.stdin.readline().strip())
prices = [int(sys.stdin.readline().strip()) for _ in range(n)]

min_price = float('inf')
max_profit = 0

for price in prices:
    if price < min_price:
        min_price = price
    else:
        profit = price - min_price
        if profit > max_profit:
            max_profit = profit

print(max_profit)
